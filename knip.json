{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "TYPES/*.ts" 
  ],
  "project": [
    "TYPES/**/*.ts"
  ],
  "ignoreBinaries": ["forge", "slither", "solc"],
  "ignoreDependencies": [
    "eslint",
    "prettier",
    "solhint",
    "type-coverage",
    "@eslint/js",
    "typescript-eslint",
    "@openzeppelin/contracts",
    "@openzeppelin/contracts-upgradeable",
    "dotenv"
  ],
  "paths": {
    "@types/*": ["TYPES/*"]
  }
}
